            <stringProp name="script">import org.apache.jmeter.gui.GuiPackage;
import org.apache.commons.io.FilenameUtils;


String testPlanFile = GuiPackage.getInstance().getTestPlanFile();
String testPlanFileDir = FilenameUtils.getFullPathNoEndSeparator(testPlanFile);
String csvDir = testPlanFileDir.replace(&apos;\\&apos;, &apos;/&apos;);

responseJsonO = vars.get(&quot;results_ALL&quot;);
responseJson = responseJsonO.replaceAll(&quot;,&quot;, &quot;\n&quot;);
log.info(responseJson);

fileOutputStream = new FileOutputStream(csvDir + &quot;/response.csv&quot;, true);
printStrean = new PrintStream(fileOutputStream); 
this.interpreter.setOut(printStrean); 
print(responseJson);
fileOutputStream.close();</stringProp>
